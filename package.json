{
	"name": "extension",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@manaflair/redux-batch": "^1.0.0",
		"@reduxjs/toolkit": "^1.7.1",
		"@tanem/react-nprogress": "^4.0.1",
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"axios": "^0.21.1",
		"dayjs": "^1.10.7",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-feather": "^2.0.9",
		"react-hot-toast": "^1.0.1",
		"react-infinite-scroll-component": "^5.1.0",
		"react-redux": "^7.2.6",
		"react-router-dom": "^5.3.0",
		"react-scripts": "^5.0.0",
		"react-tooltip": "^4.2.21",
		"styled-components": "5.1.1"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build && mv build/static/js/main*.js build/static/js/main.js",
		"build:react": "react-scripts build",
		"build:bundle": "webpack --mode production --config webpack.config.js",
		"build:extension": "yarn run build && yarn run build:bundle",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"resolutions": {
		"react-error-overlay": "6.0.10"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"env": {
			"browser": true,
			"webextensions": true
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/preset-env": "^7.12.11",
		"@babel/preset-react": "^7.12.10",
		"babel-loader": "^8.2.2",
		"babel-plugin-import": "^1.13.3"
	}
}
